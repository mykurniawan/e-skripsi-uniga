roups.plural.subscriptionManager";s:21:"Abonnementsansvarlige";s:41:"default.groups.abbrev.subscriptionManager";s:5:"AboAn";s:33:"default.genres.researchInstrument";s:20:"Forskningsinstrument";s:32:"default.genres.researchMaterials";s:19:"Forskningsmateriale";s:30:"default.genres.researchResults";s:20:"Forskningsresultater";s:26:"default.genres.transcripts";s:15:"Transkripsjoner";s:27:"default.genres.dataAnalysis";s:11:"Dataanalyse";s:22:"default.genres.dataSet";s:8:"Datasett";s:26:"default.genres.sourceTexts";s:10:"Kildetekst";s:36:"default.groups.name.externalReviewer";s:8:"Fagfelle";s:38:"default.groups.plural.externalReviewer";s:9:"Fagfeller";s:38:"default.groups.abbrev.externalReviewer";s:2:"FF";}}}