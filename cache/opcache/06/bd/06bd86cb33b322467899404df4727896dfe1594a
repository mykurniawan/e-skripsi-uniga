ayout.galleys";s:9:"Saliktņi";s:34:"submission.layout.galleyStylesheet";s:17:"Stila lapas datne";s:29:"submission.layout.galleyStyle";s:5:"Stils";s:39:"submission.layout.initialGalleyCreation";s:30:"Sākotnējā saliktņa izveide";s:28:"submission.layout.layoutFile";s:12:"Maketa datne";s:17:"submission.layout";s:6:"Makets";s:31:"submission.layout.layoutVersion";s:14:"Maketa versija";s:29:"submission.layout.noStyleFile";s:48:"Šim saliktnim nav pievienota stila lapas datne.";s:31:"submission.layout.viewingGalley";s:38:"Skatīt iesniegtā materiāla saliktni";s:27:"submission.layout.viewProof";s:15:"Skatīt paraugu";s:19:"submission.metadata";s:30:"Iesniegtā materiāla metadati";s:27:"submission.notes.addNewNote";s:24:"Pievienot jaunu piezīmi";s:29:"submission.notes.attachedFile";s:16:"Pievienota datne";s:38:"submission.notes.backToSubmissionNotes";s:37:"Atpakaļ uz piezīmēm par materiālu";s:27:"submission.notes.breadcrumb";s:9:"Piezīmes";s:30:"submission.notes.clearAllNotes";s:25:"Notīrīt visas piezīmes";s:30:"submission.notes.collapseNotes";s:18:"Sakļaut piezīmes";s:30:"submission.notes.confirmDelete";s:35:"Dzēst šo piezīmi par materiālu?";s:30:"submission.notes.createNewNote";s:23:"Izveidot jaunu piezīmi";s:27:"submission.notes.deleteNote";s:17:"Izdzēst piezīmi";s:25:"submission.notes.editNote";s:18:"Rediģēt piezīmi";s:28:"submission.notes.expandNotes";s:18:"Izvērst piezīmes";s:34:"submission.notes.noSubmissionNotes";s:28:"Nav piezīmju par materiālu";s:16:"submission.notes";s:24:"Piezīmes par materiālu";s:27:"submission.notes.updateNote";s:17:"Atjaunot piezīmi";s:26:"submission.notes.viewNotes";s:17:"Skatīt piezīmes";s:23:"submission.originalFile";s:18:"Sākotnējā datne";s:23:"submission.page.editing";s:20:"#{$id} rediģēšana";s:23:"submission.page.history";s:15:"#{$id} vēsture";s:22:"submission.page.review";s:20:"#{$id} recenzēšana";s:23:"submission.page.summary";s:19:"#{$id} kopsavilkums";s:21:"submission.peerReview";s:13:"Recenzēšana";s:25:"submission.recommendation";s:33:"R